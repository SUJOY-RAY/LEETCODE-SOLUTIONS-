class Solution:
    def countPrefixSuffixPairs(self, words: List[str]) -> int:
        counter = 0
        for i in range(len(words)):
            for j in range(i + 1, len(words)):
                word_i = words[i]
                word_j = words[j]
                if word_j.startswith(word_i) and word_j.endswith(word_i):
                    counter += 1
        return counter
