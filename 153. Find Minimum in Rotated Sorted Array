class Solution {
    public int findMin(int[] nums) {
        int left = 0, right = nums.length - 1;
        int result = Integer.MAX_VALUE;
        while (left <= right) {
            int mid = (left + right) / 2;
            result = Math.min(nums[mid], result);

            if (nums[left] <= nums[mid]) {
                result = Math.min(result, nums[left]);
                left = mid + 1;
            } else {
                right = mid - 1;
            }

        }
        return result;
    }
}
